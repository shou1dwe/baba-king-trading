# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Application.index()
GET        /strategies          controllers.Application.strategies()
GET        /strategy/view/:id          controllers.Application.strategyView(id: Long)
GET        /strategy/create          controllers.Application.strategyCreate
GET        /strategy/modify/:id          controllers.Application.strategyModify(id: Long)
GET        /strategy/remove/:id          controllers.Application.strategyRemove(id: Long)
GET        /stock/:tickerSymbol          controllers.Application.stockView(tickerSymbol: String)


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)
